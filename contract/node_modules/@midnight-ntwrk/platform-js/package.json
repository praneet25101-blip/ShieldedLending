{
  "name": "@midnight-ntwrk/platform-js",
  "version": "2.2.0",
  "description": "Provides a set of core abstractions, utilities, and types for building services and libraries that work with the Midnight blockchain",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/midnight-ntwrk/artifacts.git"
  },
  "sideEffects": [],
  "author": "IOHK",
  "dependencies": {
    "@effect/platform": "^0.94.2",
    "effect": "^3.19.15",
    "tslib": "^2.8.1"
  },
  "main": "./dist/esm/index.js",
  "types": "./dist/dts/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/dts/index.d.ts",
      "import": "./dist/esm/index.js",
      "default": "./dist/cjs/index.js"
    },
    "./effect": {
      "types": "./dist/dts/effect/index.d.ts",
      "import": "./dist/esm/effect/index.js",
      "default": "./dist/cjs/effect/index.js"
    },
    "./effect/CoinPublicKey": {
      "types": "./dist/dts/effect/CoinPublicKey.d.ts",
      "import": "./dist/esm/effect/CoinPublicKey.js",
      "default": "./dist/cjs/effect/CoinPublicKey.js"
    },
    "./effect/Configuration": {
      "types": "./dist/dts/effect/Configuration.d.ts",
      "import": "./dist/esm/effect/Configuration.js",
      "default": "./dist/cjs/effect/Configuration.js"
    },
    "./effect/ContractAddress": {
      "types": "./dist/dts/effect/ContractAddress.d.ts",
      "import": "./dist/esm/effect/ContractAddress.js",
      "default": "./dist/cjs/effect/ContractAddress.js"
    },
    "./effect/DomainSeparator": {
      "types": "./dist/dts/effect/DomainSeparator.d.ts",
      "import": "./dist/esm/effect/DomainSeparator.js",
      "default": "./dist/cjs/effect/DomainSeparator.js"
    },
    "./effect/Hex": {
      "types": "./dist/dts/effect/Hex.d.ts",
      "import": "./dist/esm/effect/Hex.js",
      "default": "./dist/cjs/effect/Hex.js"
    },
    "./effect/IntegerRange": {
      "types": "./dist/dts/effect/IntegerRange.d.ts",
      "import": "./dist/esm/effect/IntegerRange.js",
      "default": "./dist/cjs/effect/IntegerRange.js"
    },
    "./effect/NetworkId": {
      "types": "./dist/dts/effect/NetworkId.d.ts",
      "import": "./dist/esm/effect/NetworkId.js",
      "default": "./dist/cjs/effect/NetworkId.js"
    },
    "./effect/NetworkIdMoniker": {
      "types": "./dist/dts/effect/NetworkIdMoniker.d.ts",
      "import": "./dist/esm/effect/NetworkIdMoniker.js",
      "default": "./dist/cjs/effect/NetworkIdMoniker.js"
    },
    "./effect/ParseError": {
      "types": "./dist/dts/effect/ParseError.d.ts",
      "import": "./dist/esm/effect/ParseError.js",
      "default": "./dist/cjs/effect/ParseError.js"
    },
    "./effect/SigningKey": {
      "types": "./dist/dts/effect/SigningKey.d.ts",
      "import": "./dist/esm/effect/SigningKey.js",
      "default": "./dist/cjs/effect/SigningKey.js"
    },
    "./effect/index": {
      "types": "./dist/dts/effect/index.d.ts",
      "import": "./dist/esm/effect/index.js",
      "default": "./dist/cjs/effect/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "effect": [
        "./dist/dts/effect/index.d.ts"
      ],
      "effect/CoinPublicKey": [
        "./dist/dts/effect/CoinPublicKey.d.ts"
      ],
      "effect/Configuration": [
        "./dist/dts/effect/Configuration.d.ts"
      ],
      "effect/ContractAddress": [
        "./dist/dts/effect/ContractAddress.d.ts"
      ],
      "effect/DomainSeparator": [
        "./dist/dts/effect/DomainSeparator.d.ts"
      ],
      "effect/Hex": [
        "./dist/dts/effect/Hex.d.ts"
      ],
      "effect/IntegerRange": [
        "./dist/dts/effect/IntegerRange.d.ts"
      ],
      "effect/NetworkId": [
        "./dist/dts/effect/NetworkId.d.ts"
      ],
      "effect/NetworkIdMoniker": [
        "./dist/dts/effect/NetworkIdMoniker.d.ts"
      ],
      "effect/ParseError": [
        "./dist/dts/effect/ParseError.d.ts"
      ],
      "effect/SigningKey": [
        "./dist/dts/effect/SigningKey.d.ts"
      ],
      "effect/index": [
        "./dist/dts/effect/index.d.ts"
      ]
    }
  }
}